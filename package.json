{
  "name": "react-waterfall-timeline",
  "version": "1.0.1",
  "description": "A waterfall timeline component similar to Chrome DevTools Network panel for visualizing time-based data",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "react",
    "waterfall",
    "timeline",
    "chart",
    "visualization",
    "performance",
    "network",
    "devtools",
    "gantt"
  ],
  "author": "moonrailgun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moonrailgun/react-waterfall-timeline.git"
  },
  "bugs": {
    "url": "https://github.com/moonrailgun/react-waterfall-timeline/issues"
  },
  "homepage": "https://github.com/moonrailgun/react-waterfall-timeline#readme",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run storybook",
    "build": "vite build",
    "prepare": "vite build",
    "lint": "eslint .",
    "release": "npm publish --registry=https://registry.npmjs.com/",
    "prettier": "prettier --write .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/addon-vitest": "^9.1.13",
    "@storybook/react-vite": "^9.1.13",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^9.1.13",
    "globals": "^16.4.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^9.1.13",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
